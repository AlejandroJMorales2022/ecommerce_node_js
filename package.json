{
  "name": "productmanager",
  "version": "1.0.0",
  "description": "Product Manager con Express",
  "main": "server.js",
  "scripts": {
    "test_products": "mocha src/test/Products.test.js",
    "test_carts": "mocha src/test/Carts.test.js",
    "test_users": "mocha src/test/Users.test.js",
    "dev": "nodemon ./src/server.js",
    "start": "node ./src/server.js",
    "start_pf": "cross-env MANAGER_PERSISTANCE=file nodemon ./src/server.js",
    "start_pm": "cross-env MANAGER_PERSISTANCE=mongo nodemon ./src/server.js",
    "seed:products": "node ./scripts/seed.products.js",
    "artillery:simple": "artillery quick --count 40 --num 50 http://localhost:8080/simple -o simple.json",
    "artillery:compleja": "artillery quick --count 40 --num 50 http://localhost:8080/simple -o compleja.json"
  },
  "author": "Alejandro Javier Morales",
  "license": "ISC",
  "dependencies": {
    "@sendgrid/mail": "^7.7.0",
    "artillery": "^2.0.0-37",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "commander": "^11.0.0",
    "connect-mongo": "^5.0.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "crypto-random-string": "^5.0.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-handlebars": "^7.0.7",
    "express-session": "^1.17.3",
    "faker-js": "^1.0.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.4.1",
    "mongoose-paginate-v2": "^1.7.31",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.6",
    "nodemon": "^2.0.22",
    "passport": "^0.6.0",
    "passport-github": "^1.1.0",
    "passport-github2": "^0.1.12",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "session-file-store": "^1.5.0",
    "socket.io": "^4.7.1",
    "supertest": "^6.3.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "sweetalert2": "^11.7.18",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.1.0",
    "chai": "^4.3.10",
    "mocha": "^10.2.0"
  }
}
